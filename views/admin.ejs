<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Add Product</title>
    <%- include('./partials/head') %>
</head>
<body>
    <%- include('./partials/nav') %>
   
    <div class="container mt-4">
        <h5 class="card-header" style="text-align: center;">Orders</h5>
        <% orders.forEach(order => { %>
        <div class="card mt-5">
          
           
            <div class="card-body">
              

              <h5 class="card-title">Name:<%= order.name %></h5>
              <p class="card-text">Address:<%= order.address %></p>
              <p class="card-text">State:<%= order.state %></p>
              <p class="card-text">City:<%= order.city %></p>
              <p class="card-text">Pincode:<%= order.pincode %></p>
              <p class="card-text">Order Id:<%= order._id %></p>
             
            </div>
        
          </div>
          <% }); %> 
    </div>
   
   
    
    <%- include('./partials/script') %>
</body>
</html>